<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    {% block style %}
    <link rel="stylesheet" href="{{ asset('/build/css/app.css') }}">
    {% endblock %}

    {% block script %}
    <script defer src="{{ asset('/build/js/compact/compact.js') }}" data-turbolinks-eval="false"></script>
    <script defer src="{{ asset('/build/js/scattered/turbolinks.js') }}" data-turbolinks-eval="false"></script>
    <script defer src="{{ asset('/build/js/scattered/turbolinksConfig.js') }}" data-turbolinks-eval="false"></script>
    {% endblock %}
</head>
<body>
    {% include 'default/navbar.html.twig' %}

    {% block body %}{% endblock %}

    {% include 'default/footer.html.twig' %}
</body>
</html>